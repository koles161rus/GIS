{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "experimentalObjectRestSpread": true
        }
    },
    "env": {
        "amd": true,
        "browser": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "React": true,
        "ReactDOM": true,
        "Map": true,
        "Set": true,
        "WeakMap": true,
        "WeakSet": true,
        "BCN": true,
        "__ENV": true
    },
    "rules": {
        "semi": 2,
        "eqeqeq": 2,
        "strict": 2,
        "no-console": 1,
        "no-unused-vars": 0,
        "comma-dangle": 0
    }
}
